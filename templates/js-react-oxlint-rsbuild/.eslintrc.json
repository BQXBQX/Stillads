{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": "off",
    "@typescript-eslint/no-unused-vars": "error",
    "no-undef": "error",
    "no-unused-vars": "error",
    "no-var": "warn"
  }
}
