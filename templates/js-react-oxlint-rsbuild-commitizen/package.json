{
  "name": "Demo",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "rsbuild dev --open",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "lint": " oxlint -c ./.eslintrc.json && eslint ./src/**",
    "commit": "cz"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^0.4.13",
    "@rsbuild/plugin-react": "^0.4.13",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-plugin-react": "^7.34.0",
    "oxlint": "^0.2.13"
  },
  "ignorePatterns": [
    "temp.js",
    "**/vendor/*.js",
    "*.css"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}